
.home-main {
  padding: 0 30rpx 30rpx 30rpx;
}
.home-main .home-top {
  margin: 16rpx 0;
}
.home-main .home-top .input-outer {
  border: solid 2rpx #eaeaea;
  border-radius: 20rpx;
}
.home-main .home-top .input-outer input {
  padding: 8rpx 16rpx;
  font-size: 28rpx;
}
.home-main .home-main-feed {
  margin: 20rpx 0 0 0;
  padding: 10rpx;
  font-size: 13Px;
  color: #333;
  text-align: center;
  border-bottom: solid 1rpx goldenrod;
  border: solid 1rpx gainsboro;
  border-radius: 4%;
}
.home-main .home-main-feed image {
  width: 90%;
  margin: 10rpx 0;
}
